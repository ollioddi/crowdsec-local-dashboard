# Database URL for SQLite
DATABASE_URL="file:./dev.db"

# Better Auth configuration
BETTER_AUTH_SECRET= # Generate with: npx @better-auth/cli secret
# BETTER_AUTH_URL= # Optional. Set only if behind a reverse proxy where the request origin is wrong (e.g. https://dashboard.example.com)

# CrowdSec LAPI connection (include port in URL, e.g. http://192.168.10.75:8080)
LAPI_URL=
LAPI_MACHINE_ID=
LAPI_MACHINE_PASSWORD=
LAPI_BOUNCER_API_TOKEN=
LAPI_POLL_INTERVAL=60 # seconds between LAPI syncs (default: 60)
